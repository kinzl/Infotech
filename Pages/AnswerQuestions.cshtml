        <div id="u14_text" class="text ">
            <p>
                <span>Criticality</span>
            </p>
        </div>
    </div>

    <!-- Save Button -->
    <div id="u15" class="ax_default button">
        <div id="u15_div" class=""></div>
        <div id="u15_text" class="text ">
            <p>
                <span>Save</span>
            </p>
        </div>
    </div>

    <!-- Infotech Logo -->
    <div id="u16" class="ax_default image">
        <img id="u16_img" class="img " src="images/mainwindow/u4.png"/>
        <div id="u16_text" class="text " style="display:none; visibility: hidden">
            <p></p>
        </div>
    </div>

    <!-- Radio Button Group (Styled) (Group) -->

    <div id="u23" class="form-check myRadBut">
        <input class="form-check-input" type="radio" name="flexRadioDefault" id="flexRadioDefault1">
        <label class="form-check-label" for="flexRadioDefault1">
