@page
@model Questionnaire_Frontend.Pages.AnwerQuestionsModel;
@{
}
<!DOCTYPE html>
<html lang="">
<head>
    <title>AnswerQuestions</title>
    <link href="files/answerquestions/styles.css" type="text/css" rel="stylesheet"/>
    <link href="data/styles.css"/>
</head>
<body>
<div id="base">

    <!-- Unnamed (Text Field) -->
    <div id="u9" class="ax_default text_field">
        <div id="u9_div" class=""></div>
        <input id="u9_input" type="text" value="" class="u9_input"/>
    </div>

    <!-- Unnamed (Droplist) -->
    <div id="u10" class="ax_default droplist">
        <div id="u10_div" class=""></div>
        <select id="u10_input" class="u10_input">
            <option class="u10_input_option" value="Security Check Extended">Security Check Extended</option>
            <option class="u10_input_option" value="Security Check Light">Security Check Light</option>
        </select>
    </div>

    <!-- Save Button -->
    <div id="u15" class="ax_default button">
        <div id="u15_div" class=""></div>
        <div id="u15_text" class="text ">
            <a asp-page-handler="Any">Save</a>
        </div>

    </div>

    <!-- Infotech Logo -->
    <div id="u16" class="ax_default image">
        <img id="u16_img" class="img " src="images/mainwindow/u4.png"/>
        <div id="u16_text" class="text " style="display:none; visibility: hidden">
            
        </div>
    </div>

    @* Generate Questions dynamically *@
    <script src="js/AnswerQuestionsGeneratedTags.js" asp-append-version="true"></script>
   
    <button id="MyButton" class="myButtonNext">MyNextButton</button>
</div>

</body>
</html>