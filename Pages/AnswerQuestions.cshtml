        <textarea id="u5_input" class="u5_input"></textarea>
    </div>

    <!-- Unnamed (Rectangle) -->
    <div id="u6" class="ax_default label">
        <div id="u6_div" class=""></div>
        <div id="u6_text" class="text ">
            <p>
                <span>Question 1</span>
            </p>
        </div>
    </div>

    <!-- Unnamed (Shape) -->
    <div id="u7" class="ax_default icon">
        <a asp-page-handler="RedirectToUpdateSecurityCheck">
            <img id="u7_img" class="img " src="images/answerquestions/u7.svg"/>
            <div id="u7_text" class="text " style="display:none; visibility: hidden">
            </div>
        </a>
    </div>

    <!-- Unnamed (Rectangle) -->
    <div id="u8" class="ax_default label">
        <div id="u8_div" class=""></div>
        <div id="u8_text" class="text ">
